{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "Code Pad",
  "description": "A JavaScript editor for Chrome OS",
  "app": {
    "background": {
      "scripts": [
        "src/scripts/native/main.js"
      ]
    }
  },
  "file_browser_handlers": [
    {
      "id": "open-file",
      "default_title": "Open file"
    }
  ],
  "web_accessible_resources": [
    "src/html/templates/*"
  ],
  "permissions": [
    "http://fonts.gstatic.com/",
    "https://fonts.gstatic.com/",
    "fileSystem"
  ]
}